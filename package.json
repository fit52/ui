{
  "name": "fit52-site",
  "version": "0.1.0",
  "private": true,
  "repository": "https://github.com/fit52/ui",
  "bugs": "https://github.com/fit52/ui/issues",
  "proxy": "http://localhost:3001/",
  "engines": {
    "node": ">=8.10.0",
    "npm": "5.3.0"
  },
  "dependencies": {
    "@cloudant/cloudant": "^2.4.0",
    "body-parser": "^1.18.3",
    "config": "^2.0.1",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "wpcom": "^5.4.1"
  },
  "scripts": {
    "precommit": "yarn lint",
    "prepush": "yarn lint",
    "lint": "eslint src/**/*.js server/**/*.js",
    "start:dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start": "node server/index.js",
    "dev": "concurrently --kill-others-on-fail \"NODE_ENV=development yarn start\" \"yarn start:dev\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn lint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 1 versions",
    "Firefox ESR",
    "not ie <= 10"
  ],
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "carbon-components-react": "^5.52.1",
    "concurrently": "^3.5.1",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "react-spinners": "^0.3.3"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "airbnb"
    ],
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-filename-extension": 0,
      "react/jsx-one-expression-per-line": 0,
      "import/no-extraneous-dependencies": 0,
      "react/forbid-prop-types": 0,
      "no-restricted-syntax": 0,
      "no-await-in-loop": 0
    }
  },
  "eslintIgnore": [
    "package.json",
    "node_modules",
    "build",
    "lib",
    "__snapshots__",
    "*.test.js",
    "registerServiceWorker.js"
  ]
}
